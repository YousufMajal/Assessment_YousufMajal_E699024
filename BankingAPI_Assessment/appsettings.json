{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=DB_HOST_SERVER;Database=DB_NAME;User=DB_USERNAME;Password=DB_PASSWORD;Trusted_Connection=False;Encrypt=True;TrustServerCertificate=True;"
  },
  "Config": {
    "Aws": {
      "Region": "us-east-1",
      "AccessKeyId": null,
      "SecretAccessKey": null,
      "SessionToken": null,
      "WithdrawalTopicArn": "arn:aws:sns:us-east-1:123456789012:withdrawal-events"
    },
    "WithdrawalEvents": {
      "TopicArn": "arn:aws:sns:us-east-1:123456789012:WithdrawalTopic",
      "Subject": "WithdrawalEvent"
    },
    "OutboxProcessor": {
      "ProcessingIntervalSeconds": 30,
      "BatchSize": 10,
      "DefaultBatchSize": 50
    },
    "BusinessRules": {
      "MaxWithdrawalAmount": 1000000.0,
      "MinWithdrawalAmount": 0.01
    }
  }
}